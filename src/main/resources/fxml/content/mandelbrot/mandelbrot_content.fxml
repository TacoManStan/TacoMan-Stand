<?xml version="1.0" encoding="UTF-8"?>

<?import com.taco.suit_lady.view.ui.jfx.components.BoundCanvas?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="500.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.taco.suit_lady.view.ui.ui_internal.contents_sl.mandelbrot.SLMandelbrotContentController">
    <children>
        <BorderPane fx:id="borderPaneRoot" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
            <top>
            <BorderPane BorderPane.alignment="CENTER">
               <center>
                      <Label fx:id="titleLabel" text="Mandelbrot Set Demo">
                     <font>
                        <Font name="System Bold" size="24.0" />
                     </font>
                  </Label>
               </center>
            </BorderPane>
            </top>
         <center>
            <StackPane fx:id="canvasStackPane" BorderPane.alignment="CENTER">
               <children>
                  <BoundCanvas fx:id="canvas" />
               </children>
            </StackPane>
         </center>
         <bottom>
            <ProgressBar fx:id="progressBar" prefHeight="18.0" prefWidth="500.0" progress="0.0" BorderPane.alignment="CENTER" />
         </bottom>
        </BorderPane>
    </children>
</AnchorPane>
